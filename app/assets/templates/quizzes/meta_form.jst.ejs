  <div class="panel-heading" style="text-align:center">
    <h2>
      <div class="panel-title">Choose a title and time limit
      </div>
    </h2>
  </div>

  <div class="panel-body">
    <div class='container'>
      <div class='row'>
        <div class='col-xs-1'>
          <button type="button" class="btn btn-primary btn-lg btn-prev" style="margin:30px">
            Prev
          </button>
        </div>
         <div class="col-xs-4 col-xs-offset-1 form-group">
             <label> Title </label>
             <input type="text" class="form-control" id="quiz-title" placeholder="Enter title">
         </div>
         <div class='col-xs-1 col-xs-offset-3'>
           <button type="button" class="btn btn-primary btn-lg btn-next" style="margin:30px">
             Next
           </button>
         </div>
      </div>
      <div class='row'>
         <div class="col-xs-2 col-xs-offset-2 form-group">
             <label> Time </label>
               <input type="text" class="form-control" id="quiz-minutes" placeholder="Minutes">
               <input type="text" class="form-control" id="quiz-seconds" placeholder="Seconds">
         </div>
            <div class="col-xs-6 col-xs-offset-1 form-group">
             <label> Description </label>
               <textarea type="text" class="form-control" id="quiz-description" placeholder="Description"></textarea>
         </div>
      </div>
    <div class='row'>
        <div class='col-xs-8 col-xs-offset-2 form-group'> 
            <label> Column Headers </label>
              <input type="text" class="form-control" id="hint-header" placeholder="Hint Header">
               <input type="text" class="form-control" id="answer-header" placeholder="Answer Header">
        </div>     
    </div>
  </div>



